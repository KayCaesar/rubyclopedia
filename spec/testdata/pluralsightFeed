    <link rel="http://schemas.microsoft.com/ado/2007/08/dataservices/related/Modules" type="application/atom+xml;type=feed" title="Modules" href="Courses('Windows%20Azure%20Infrastructure%20as%20a%20Service%20Essentials')/Modules" />
