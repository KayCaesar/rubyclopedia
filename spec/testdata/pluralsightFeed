        <d:IsTutorial m:type="Edm.Boolean">false</d:IsTutorial>
        <d:Name>windows8-contracts</d:Name>
        <d:ShortDescription>Working with contracts such as Search and Share and some less common extensions such as AutoPlay.</d:ShortDescription>
        <d:Subcategory></d:Subcategory>
